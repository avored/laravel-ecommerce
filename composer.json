{
	"name" : "avored/laravel-ecommerce",
	"description" : "AvoRed E Commerce is an Laravel Open Source Shopping Cart",
	"keywords" : [
		"framework",
		"laravel",
		"ecommerce",
		"laravel 5",
		"shop",
		"shopping-cart",
		"e-commerce",
		"shopping cart",
		"e commerce"
	],
	"license" : "MIT",
	"type" : "project",
	"require" : {
		"php": ">=7.1.3",
		"avored/banner": "~1.5",
		"avored/brand": "~1.5",
		"avored/cash-on-delivery": "~1.2",
		"avored/contact": "~1.2",
		"avored/dummy-data": "~1.4",
		"avored/ecommerce": "~1.6",
		"avored/featured": "~1.7",
		"avored/fixed-rate": "~1.5",
		"avored/module-installer": "1.*",
		"avored/pickup": "~1.5",
		"avored/related": "~1.3",
		"avored/review": "~1.3",
		"avored/subscribe": "~1.3",
		"fabpot/goutte": "^3.2",
		"fideloper/proxy": "~4.0"
	},
	"require-dev" : {
		"filp/whoops" : "~2.0",
		"nunomaduro/collision" : "~1.1",
		"fzaninotto/faker" : "~1.4",
		"mockery/mockery" : "~1.0",
		"phpunit/phpunit" : "~7.0",
		"symfony/thanks" : "^1.0"
	},
	"autoload" : {
		"classmap" : [
			"database/seeds",
			"database/factories"
		],
		"psr-4" : {
			"App\\" : "app/"
		}
	},
	"autoload-dev" : {
		"psr-4" : {
			"Tests\\" : "tests/"
		}
	},
	"scripts" : {
		"post-root-package-install" : "@php -r \"file_exists('.env') || copy('.env.example', '.env');\"",
		"post-create-project-cmd" : "@php artisan key:generate",
		"post-autoload-dump" : [
			"Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
			"@php artisan package:discover"
		]
	},
	"config" : {
		"preferred-install" : "dist",
		"sort-packages" : true,
		"optimize-autoloader" : true
	},
	"homepage" : "https://www.avored.com",
	"support" : {
		"email" : "ind.purvesh@gmail.com",
		"issues" : "https://www.avored.com/discussion",
		"forum" : "https://www.avored.com/discussion",
		"wiki" : "https://www.avored.com/docs",
		"source" : "https://www.github.com/avored/laravel-ecommerce"
	}
}
